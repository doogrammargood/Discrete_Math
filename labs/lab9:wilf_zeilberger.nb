Cell[TextData[{
 "Note that F(k,n)=0, except when k is in [0,n], so f(n) is the summation of \
F(n,k) over all integers k.\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         RowBox[{"(", 
           RowBox[{
             StyleBox["n", "TI"], "+", "2"}], ")"}], 
         RowBox[{"(", 
           RowBox[{
             StyleBox["n", "TI"], "-", 
             StyleBox["x", "TI"]}], ")"}], 
         StyleBox["f", "TI"], 
         RowBox[{"(", 
           StyleBox["n", "TI"], ")"}], "-", 
         RowBox[{"(", 
           RowBox[{
             StyleBox["n", "TI"], "+", "2"}], ")"}], 
         RowBox[{"(", 
           RowBox[{
             StyleBox["n", "TI"], "-", 
             StyleBox["x", "TI"]}], ")"}], 
         StyleBox["f", "TI"], 
         RowBox[{"(", 
           RowBox[{
             StyleBox["n", "TI"], "+", "2"}], ")"}], "\[LongEqual]", "0"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "(n+2)(n-x)f(n)-(n+2)(n-x)f(n+2)=0", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "3cc1221d-9d8f-4251-bd6c-ed74145521c9"],
 ". As long as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    TemplateBox[<|"boxes" -> FormBox[
