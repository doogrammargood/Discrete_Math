  3.9651394182478123`*^9}},ExpressionUUID->"dceffdc3-863a-40fb-972b-\
54f0921642ae"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"rule", " ", "=", " ", 
  RowBox[{
   RowBox[{
    RowBox[{"!", "p"}], "&&", 
    RowBox[{"!", "q"}]}], "->", 
   RowBox[{"!", 
    RowBox[{"(", 
     RowBox[{"p", "||", "q"}], ")"}]}]}]}]], "Input",
 CellChangeTimes->{{3.965139440614072*^9, 3.965139470418324*^9}, {
  3.965384645678483*^9, 
  3.965384648021377*^9}},ExpressionUUID->"c50327f3-01e9-4ac7-b453-\
bf20e1f4bf0c"],

