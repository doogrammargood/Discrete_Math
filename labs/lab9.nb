Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "r"}], "+", "s"}], ")"}], " ", 
     TagBox[
      RowBox[{"F", "[", 
       RowBox[{"j", ",", "r"}], "]"}],
      HoldForm]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "m"}], "+", "r"}], ")"}], " ", 
     TagBox[
