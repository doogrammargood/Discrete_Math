Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["G", "TI"], "1"], "\[LongEqual]", 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              StyleBox["V", "TI"], "1"], ",", 
             SubscriptBox[
              StyleBox["E", "TI"], "1"]}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> "G_1=(V_1,E_1)", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "95bc3a1f-c6c8-4f6b-a4cb-baa9bbe263da"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["G", "TI"], "2"], "\[LongEqual]", 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              StyleBox["V", "TI"], "2"], ",", 
             SubscriptBox[
              StyleBox["E", "TI"], "2"]}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> "G_2=(V_2,E_2)", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "0fb8e6ff-bdf6-4152-9615-30a00fee80ae"],
 " be graphs. We assume ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["V", "TI"], "1"], "\[Intersection]", 
         SubscriptBox[
          StyleBox["V", "TI"], "2"], "\[LongEqual]", "\[EmptySet]"}], 
       TraditionalForm], "errors" -> {}, "input" -> "V_1\\cap V_2=\\emptyset",
      "state" -> "Boxes"|>,
