  "d09d6ea0-4dc6-4c24-90f3-1cf7b6b1f3c0"],
 ". The",
 StyleBox[" disjoint union",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["G", "TI"], "1"], "\[Union]", 
         SubscriptBox[
          StyleBox["G", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "G_1\\cup G_2", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "58b0418b-336a-4731-a270-03c2b7ca936e"],
 " is the graph whose vertex set is ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["V", "TI"], "1"], "\[Union]", 
         SubscriptBox[
          StyleBox["V", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
     "input" -> "V_1\\cup V_2", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "8ce655a6-d385-4866-ac5d-01dec769fd32"],
 ", and whose edge set is ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["v", "TI"], "\[Tilde]", 
         StyleBox["w", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "v\\sim w", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5ac39b63-e1b3-4465-9459-6fb26138451e"],
 " if ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["v", "TI"], ",", 
