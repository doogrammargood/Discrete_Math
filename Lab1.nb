  RowBox[{
   RowBox[{"(", " ", 
    RowBox[{
     RowBox[{"p_", "||", "q_"}], " ", "/;", " ", 
     RowBox[{"LiteralsoutOfOrder", "[", 
      RowBox[{"p", ",", "q"}], "]"}]}], ")"}], "->", 
   RowBox[{"q", "||", "p"}]}]}]}], "Input",
 CellChangeTimes->{{3.946411290520255*^9, 3.9464113454521627`*^9}, {
