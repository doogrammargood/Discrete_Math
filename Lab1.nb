       RowBox[{"!", "q"}]}]}], ",", " ", 
     RowBox[{"(*", 
      RowBox[{"De", " ", "Morgan"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"!", 
       RowBox[{"(", 
        RowBox[{"p_", "&&", "q_"}], ")"}]}], "->", 
      RowBox[{
       RowBox[{"!", "p"}], "||", 
       RowBox[{"!", "q"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"(", " ", 
       RowBox[{
        RowBox[{"p_", "||", "q_"}], " ", "/;", " ", 
        RowBox[{"LiteralsoutOfOrder", "[", 
         RowBox[{"p", ",", "q"}], "]"}]}], ")"}], "->", 
      RowBox[{"q", "||", "p"}]}], ",", 
     RowBox[{"(*", "Commutative", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"(", " ", 
       RowBox[{
        RowBox[{"p_", "&&", "q_"}], " ", "/;", " ", 
        RowBox[{"LiteralsoutOfOrder", "[", 
         RowBox[{"p", ",", "q"}], "]"}]}], ")"}], "->", 
      RowBox[{"q", "&&", "p"}]}], ",", 
     RowBox[{"(*", "Commutative", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"p_", "||", 
       RowBox[{"(", 
        RowBox[{"q_", "&&", "r_"}], ")"}]}], "->", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"p", "||", "q"}], ")"}], "&&", 
       RowBox[{"(", 
        RowBox[{"p", "||", "r"}], ")"}]}]}], ",", " ", 
     RowBox[{"(*", 
      RowBox[{"Distributive", " ", "Rules"}], "*)"}], "\[IndentingNewLine]", 
