   RowBox[{"a_", ",", "b_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"Length", "[", "a", "]"}], "<", "3"}], "&&", 
   RowBox[{
    RowBox[{"Length", "[", "b", "]"}], "<", "3"}], "&&", 
   RowBox[{
    RowBox[{"Order", "[", 
     RowBox[{"p", ",", "q"}], "]"}], "==", 
    RowBox[{"-", "1"}]}]}]}], "\[IndentingNewLine]", 
