     "]"}]}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "We", " ", "create", " ", "a", " ", "list", " ", "of", " ", "pairs"}], 
    ",", " ", 
    RowBox[{
    "where", " ", "each", " ", "rule", " ", "is", " ", "paired", " ", "with", 
     " ", "its", " ", 
     RowBox[{"pattern", "."}]}]}], "*)"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "ApplyInferenceStep", " ", "creates", " ", "a", " ", "new", " ", "list", 
