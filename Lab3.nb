Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"inferenceRules", " ", "=", " ", 
    RowBox[{"{", 
     RowBox[{
     "modusPonens", ",", "modusTollens", ",", "simplification", ",", 
      "conjunction", ",", "hypotheticalSyllogism", ",", 
      "disjunctiveSyllogism", ",", "resolution"}], "}"}]}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "We", " ", "put", " ", "the", " ", "list", " ", "of", " ", "functions", 
