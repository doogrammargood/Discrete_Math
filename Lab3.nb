      RowBox[{"{", 
       RowBox[{
        RowBox[{"p_", "&&", "q_"}], ",", 
        RowBox[{"p_", "&&", "q_"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"p_", ",", "q_"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"p_", "\[Implies]", "q_"}], ",", 
        RowBox[{"q_", "\[Implies]", "r_"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"p_", "||", "q_"}], ",", 
        RowBox[{"!", "p_"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"p_", "||", "q_"}], ",", 
        RowBox[{
         RowBox[{"!", "p_"}], "||", "r_"}]}], "}"}]}], "}"}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{
   "These", " ", "are", " ", "the", " ", "corresponding", " ", "patterns"}], 
