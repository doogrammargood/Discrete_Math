    RowBox[{"omitting", " ", 
     RowBox[{"addition", "."}]}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"patterns", " ", "=", 
    RowBox[{"{", " ", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"p_", ",", 
        RowBox[{"p_", "\[Implies]", "q_"}]}], "}"}], ",", " ", 
      RowBox[{"{", 
