     RowBox[{"far", ".", "\[IndentingNewLine]", "In"}], " ", "more", " ", 
     "detail", " ", "from", " ", "the", " ", "inside", " ", 
     RowBox[{"out", ":", " ", "\[IndentingNewLine]", 
      RowBox[{
      "We", " ", "look", " ", "at", " ", "every", " ", "pair", " ", "of", " ",
        "propositions", " ", "in", " ", "the", " ", 
